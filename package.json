{
  "name": "dispowser",
  "version": "0.1.4",
  "description": "Tools easing the management of resource disposing. Easy to use, will support more and more use cases.",
  "main": "lib/dispowser.min.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack -p --config 'build/webpack.config.js'",
    "dev": "cross-env NODE_ENV=development webpack --config 'build/webpack.config.js'",
    "analyze": "cross-env NODE_ENV=analyze webpack --config 'build/webpack.config.js'",
    "test": "cross-env NODE_ENV=testing karma start 'build/karma.config.js'",
    "test-watch": "cross-env NODE_ENV=testing karma start 'build/karma.config.js' --browsers=Chrome --single-run=false --auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeefuji/dispowser.git"
  },
  "keywords": [
    "disposer",
    "disposable",
    "dispowser",
    "releasing",
    "release",
    "clean",
    "cleanup",
    "event",
    "events"
  ],
  "author": "Jean-Hugues Gilbert",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jeefuji/dispowser/issues"
  },
  "homepage": "https://github.com/jeefuji/dispowser#readme",
